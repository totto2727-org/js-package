{
  "name": "@totto2727/remark-link-card",
  "author": "totto2727",
  "description": "Remark plugin to convert text links to link cards inspired by gatsby-remark-link-card",
  "keywords": [
    "unified",
    "remark",
    "remark-plugin",
    "markdown",
    "link",
    "card"
  ],
  "license": "MIT",
  "version": "1.3.1",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": ["src", "dist", "package.json", "LICENSE"],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/import/index.d.mts",
        "default": "./dist/import/index.mjs"
      },
      "require": {
        "types": "./dist/require/index.d.cts",
        "default": "./dist/require/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "run-z build:transpile,build:esm,build:cjs",
    "build:transpile": "bun build/build.mts",
    "build:esm": "tsc --project build/tsconfig.build.esm.json",
    "build:cjs": "tsc --project build/tsconfig.build.cjs.json",
    "prepublish": "attw --pack . || exit 0"
  },
  "dependencies": {
    "he": "^1.2.0",
    "open-graph-scraper": "git@github.com:totto2727-org/openGraphScraper.git",
    "remeda": "^1.46.0",
    "sanitize-filename": "^1.6.3",
    "unist-util-visit": "^2.0.3"
  },
  "devDependencies": {
    "@remark-embedder/core": "^1.2.4",
    "@types/he": "^1.2.3",
    "next-mdx-remote": "^2.1.4",
    "node-html-parser": "^3.1.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1"
  }
}
