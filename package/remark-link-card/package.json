{
  "name": "@totto2727/remark-link-card",
  "author": "totto2727",
  "description": "Remark plugin to convert text links to link cards inspired by gatsby-remark-link-card",
  "keywords": [
    "unified",
    "remark",
    "remark-plugin",
    "markdown",
    "link",
    "card"
  ],
  "license": "MIT",
  "version": "1.3.1",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/import/index.d.mts",
        "default": "./dist/import/index.mjs"
      },
      "require": {
        "types": "./dist/require/index.d.cts",
        "default": "./dist/require/index.cjs"
      }
    }
  },
  "files": ["src", "dist", "package.json", "tsconfig.json", "LICENSE"],
  "scripts": {
    "build": "run-z build:transpile,build:esm,build:cjs",
    "build:transpile": "bun build/build.mts",
    "build:esm": "tsc --project build/tsconfig.build.esm.json",
    "build:cjs": "tsc --project build/tsconfig.build.cjs.json",
    "check": "run-z check:biome,check:tsc",
    "check:biome": "biome check .",
    "check:tsc": "tsc --noEmit",
    "fix": "run-z fix:biome",
    "fix:biome": "biome check --apply-unsafe .",
    "test": "bun test",
    "prepublish": "attw --pack . || exit 0"
  },
  "dependencies": {
    "@totto2727/result": "workspace:*",
    "he": "^1.2.0",
    "open-graph-scraper": "^6.5.0",
    "remark": "^15.0.1",
    "remeda": "^1.46.0",
    "unified": "^11.0.0",
    "unist-util-visit": "^2.0.3",
    "url-regex-safe": "^4.0.0"
  },
  "devDependencies": {
    "@remark-embedder/core": "^3.0.2",
    "@types/he": "^1.2.3",
    "@types/url-regex-safe": "^1.0.2",
    "remark-html": "^16.0.1"
  }
}
